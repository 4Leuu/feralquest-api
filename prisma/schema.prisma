generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  name String
  email String @unique
  password String
  cpf String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  role Role
  phone Int?
}

enum Role {
  user 
  admin
}

